#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/ryanbowz/Documents/xcode/CSCE689/glfw-3.3.8/src
  /Applications/CMake.app/Contents/bin/cmake -P /Users/ryanbowz/Documents/xcode/CSCE689/glfw-3.3.8/CMake/GenerateMappings.cmake mappings.h.in mappings.h
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/ryanbowz/Documents/xcode/CSCE689/glfw-3.3.8/src
  /Applications/CMake.app/Contents/bin/cmake -P /Users/ryanbowz/Documents/xcode/CSCE689/glfw-3.3.8/CMake/GenerateMappings.cmake mappings.h.in mappings.h
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/ryanbowz/Documents/xcode/CSCE689/glfw-3.3.8/src
  /Applications/CMake.app/Contents/bin/cmake -P /Users/ryanbowz/Documents/xcode/CSCE689/glfw-3.3.8/CMake/GenerateMappings.cmake mappings.h.in mappings.h
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/ryanbowz/Documents/xcode/CSCE689/glfw-3.3.8/src
  /Applications/CMake.app/Contents/bin/cmake -P /Users/ryanbowz/Documents/xcode/CSCE689/glfw-3.3.8/CMake/GenerateMappings.cmake mappings.h.in mappings.h
fi

